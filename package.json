{
  "name": "e-commerce-test-framework",
  "version": "1.0.0",
  "description": "cypress test automation framework",
  "main": "index.js",
  "scripts": {
    "reports-junit:delete": "node delete-junit-reports.mjs",
    "reports-junit:combine": "jrm cypress/reports/junit/combined-report.xml \"cypress/reports/junit/*.xml\"",
    "report:pre": "npm run reports-junit:delete",
    "report:post": "npm run reports-junit:combine",
    "test:report": "npm run report:pre && npx cypress run && npm run report:post",
    "test:run": "npx cypress run",
    "test:chrome": "npx cypress run --browser chrome",
    "test:firefox": "npx cypress run --browser firefox",
    "test:edge": "npx cypress run --browser edge",
    "generate-fake-data": "node ./scripts/generateFakeData.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sylvestre01/E-commerce-Checkout-Test.git"
  },
  "keywords": [
    "cypress"
  ],
  "author": "Sylvester",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sylvestre01/E-commerce-Checkout-Test/issues"
  },
  "homepage": "https://github.com/Sylvestre01/E-commerce-Checkout-Test#readme",
  "devDependencies": {
    "@cypress/grep": "^3.1.5",
    "@faker-js/faker": "^8.0.2",
    "cypress": "^13.15.0",
    "cypress-mochawesome-reporter": "^3.5.1",
    "cypress-multi-reporters": "^1.6.3",
    "cypress-xpath": "^2.0.1",
    "junit-report-merger": "^7.0.0",
    "mkdirp": "^3.0.1",
    "mocha-junit-reporter": "^2.2.1",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "winston": "^3.15.0"
  }
}
